{
  "installCommand": "npm install",
  "buildCommand": "echo 'No build required for Edge Functions'",
  "crons": [
    { "path": "/api/cron/check-reminders", "schedule": "0 9 * * *" }
  ],
  "headers": [
    {
      "source": "/api/whatsapp/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store"
        }
      ]
    }
  ]
}


